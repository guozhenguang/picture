        <mxCell id="R4_T0SXZJ25gIyN9d9QS-11" value="直接 new 一个 android::impl::HWComposer" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="1">
