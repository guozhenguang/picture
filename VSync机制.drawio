          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="6zomOD1P-mL0mL0aaMnA-21" value="初始化了一个 &lt;code style=&quot;color: rgb(30, 107, 184); font-size: 14px; line-height: 1.8em; letter-spacing: 0em; background: none 0% 0% / auto no-repeat scroll padding-box border-box rgba(27, 31, 35, 0.05); width: auto; height: auto; margin-left: 2px; margin-right: 2px; padding: 2px 4px; border-style: none; border-width: 3px; border-color: rgb(0, 0, 0) rgba(0, 0, 0, 0.4) rgba(0, 0, 0, 0.4); border-radius: 4px; font-family: Consolas, Monaco, Menlo, monospace; word-break: break-all;&quot;&gt;renderengine::RenderEngineCreationArgs::Builder&lt;/code&gt;&amp;nbsp;&lt;div&gt;对象，接着将 builder 对象传入&lt;b&gt; create &lt;/b&gt;函数构建一个 RenderEngine 对象&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="1" vertex="1">
