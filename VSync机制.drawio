          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="R4_T0SXZJ25gIyN9d9QS-11" value="直接 new 一个 android::impl::HWComposer" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="1" vertex="1">
